class main{

integer factorial(integer n){

        
	integer i;         
        integer total;

        total = 1;
        i = 4;
	while(i > 0){

		total += total * i;  
		i = i - 1;

	}
	return total;

} 
void printI(integer i)extern;

void  main (){
  printI(factorial(5.0)); //Error factorial recibe integer como parametro.
 
  return factorial(2); //Error el main es de tipi void

}

} // end class
