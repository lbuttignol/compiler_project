class main{

/* test_potencia :funcion que calcula la x ^ n , x elevado a la n  */

void printF() extern;
float potencia(float x, integer n){

	integer i; 
	bool neg;
	float prod;
        neg = false;
        i = 1;
        prod = 1.0;

	if (n < 0){
		n = -n;
		neg = !neg;
	}

	while(i <= n){
		prod = prod * x;
		i = i + 1;
	}
	if (!(!neg)){
		return 1.0/prod;
        }
	else{
		return prod;
        }
	return -100.1;
}


void main(){
     float f;
     float a;
     a = 8.0;
     f = potencia(a, 2);
     //printF(f);
     return;
}

}
